char **split(char separator, char *at){
	char **splited=NULL;
	int j=0,k=0,count=0;
	for(int i=0;i<strlen(at);i++){
		if(at[i]==separator) count++;
	}
	splited=(char**)calloc(sizeof(char*),count+1);
	for(int i=0;i<count+1;i++){
		*(splited+i)=(char*)calloc(1,50);
	}
	for(int i=0;i<strlen(at);i++){
		if(at[i]==separator){
			k++;j=0;
		}
		else{
			*(*(splited+k)+j) = at[i];
			j++;
		}
	}
	return splited;
}
